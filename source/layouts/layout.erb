    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    
    <meta property="og:title" content="RudolfvanGorsel.nl" />
    <meta property="og:description" content="Dienstverlening in vastgoed" />
